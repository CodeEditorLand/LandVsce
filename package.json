{
	"bin": {
		"vsce": "vsce"
	},
	"contributors": [
		"Microsoft Corporation"
	],
	"dependencies": {
		"@vscode/vsce-sign": "2.0.5",
		"chalk": "5.3.0",
		"cheerio": "1.0.0",
		"cockatiel": "3.2.1",
		"commander": "12.1.0",
		"form-data": "4.0.1",
		"glob": "11.0.0",
		"hosted-git-info": "8.0.0",
		"jsonc-parser": "3.3.1",
		"leven": "4.0.0",
		"markdown-it": "14.1.0",
		"mime": "4.0.4",
		"minimatch": "10.0.1",
		"parse-semver": "1.1.1",
		"read": "4.0.0",
		"semver": "7.6.3",
		"tmp": "0.2.3",
		"typed-rest-client": "2.1.0",
		"url-join": "5.0.0",
		"xml2js": "0.6.2",
		"yauzl": "3.1.3",
		"yazl": "3.1.0"
	},
	"description": "VS Code Extensions Manager",
	"devDependencies": {
		"@types/cheerio": "0.22.35",
		"@types/glob": "8.1.0",
		"@types/hosted-git-info": "3.0.5",
		"@types/markdown-it": "14.1.2",
		"@types/mime": "4",
		"@types/minimatch": "5.1.2",
		"@types/node": "22.8.1",
		"@types/read": "0.0.32",
		"@types/semver": "7.5.8",
		"@types/tmp": "0.2.6",
		"@types/url-join": "4.0.3",
		"@types/xml2js": "0.4.14",
		"@types/yauzl": "2.10.3",
		"@types/yazl": "2.4.5",
		"source-map-support": "0.5.21"
	},
	"main": "out/api.js",
	"mocha": {
		"require": [
			"ts-node/register"
		],
		"spec": "src/test/**/*.ts",
		"watch-files": "src/**"
	},
	"name": "@vscode/vsce",
	"optionalDependencies": {
		"keytar": "7.9.0"
	},
	"scripts": {
		"api": "api-extractor run --local --verbose",
		"build": "npm run compile && npm run api",
		"compile": "tsc",
		"watch:build": "npm run compile -- --watch",
		"watch:test": "npm run test -- --watch"
	},
	"typings": "dist/vsce.d.ts"
}
